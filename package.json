{
	"name": "fixture-riveter",
	"version": "2.3.1",
	"description": "Fixture replacement inspired by thoughtbot's fixture_bot",
	"author": "Batterii, LLC",
	"contributors": [
		{
			"name": "Noah Bogart",
			"email": "nbtheduke@gmail.com",
			"url": "https://github.com/noahtheduke"
		}
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "http://github.com/Batterii/fixture-riveter.git"
	},
	"files": [
		"dist/lib",
		"dist/types"
	],
	"scripts": {
		"prepublish": "tsc",
		"build": "tsc",
		"clean": "rm -rf dist",
		"coverage": "tsc && nyc --reporter=html ts-mocha test/**/*.ts",
		"docs:build": "vuepress build docs",
		"docs:dev": "vuepress dev docs",
		"eslint": "eslint . --ext .ts --cache",
		"lint": "eslint . --ext .ts --cache; markdownlint docs/",
		"lintf": "eslint . --ext .ts --fix --cache; markdownlint docs/ --fix",
		"markdownlint": "markdownlint docs/",
		"stryker": "stryker run",
		"test": "tsc && ts-mocha test/**/*.ts",
		"test:unit": "tsc && ts-mocha test/unit/**/*.ts",
		"test:acceptance": "tsc && ts-mocha test/acceptance/**/*.ts"
	},
	"main": "dist/lib/index.js",
	"types": "dist/types/index.d.ts",
	"devDependencies": {
		"@batterii/eslint-config-ts": "^3.1.0",
		"@stryker-mutator/core": "^5.0.0",
		"@stryker-mutator/mocha-runner": "^5.0.0",
		"@stryker-mutator/typescript-checker": "^5.0.0",
		"@types/chai": "^4.2.11",
		"@types/chai-as-promised": "^7.1.2",
		"@types/fs-extra": "^9.0.8",
		"@types/knex-cleaner": "^1.3.0",
		"@types/lodash": "^4.14.155",
		"@types/mocha": "^7.0.2",
		"@types/node": "^14.14.34",
		"@types/sinon": "^7.5.2",
		"@types/sinon-chai": "^3.2.4",
		"@typescript-eslint/eslint-plugin": "^4.13.0",
		"@typescript-eslint/parser": "^4.13.0",
		"chai": "^4.2.0",
		"chai-as-promised": "^7.1.1",
		"eslint": "^7.18.0",
		"fs-extra": "^9.1.0",
		"knex": "^0.21.17",
		"knex-cleaner": "^1.3.0",
		"markdownlint": "^0.23.1",
		"mocha": "^7.2.0",
		"nyc": "^15.1.0",
		"objection": "^2.2.0",
		"objection-table-name": "^1.2.3",
		"sequelize": "^6.2.4",
		"sinon": "^7.5.0",
		"sinon-chai": "^3.5.0",
		"source-map-support": "^0.5.19",
		"sqlite3": "^5.0.0",
		"ts-mocha": "^8.0.0",
		"ts-node": "^9.1.1",
		"typescript": "^4.2.4",
		"vuepress": "^2.0.0-beta.13"
	},
	"dependencies": {
		"fast-glob": "^3.2.5",
		"lodash": "^4.17.21"
	}
}
