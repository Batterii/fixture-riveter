{
	"name": "@batterii/factory-builder",
	"version": "0.0.0",
	"description": "Fixture replacement inspired by thoughtbot's factory_bot",
	"author": "Batterii, LLC",
	"contributors": [
		{
			"name": "Noah Bogart",
			"email": "nbtheduke@gmail.com",
			"url": "https://github.com/noahtheduke"
		}
	],
	"license": "MIT",
	"repository": "github:Batterii/factory-builder",
	"files": [
		"dist/lib",
		"dist/types"
	],
	"scripts": {
		"generate": "yo --local-only",
		"generate:class": "yo @batterii/ts-class  --local-only ",
		"generate:module": "yo @batterii/ts-module --local-only ",
		"generate:test": "yo @batterii/ts-test --local-only ",
		"lint": "eslint . --ext .ts --cache",
		"lintf": "eslint . --ext .ts --fix --cache",
		"build": "rm -rf dist && tsc && tsc --project tsconfig-types.json",
		"clean": "rm -rf dist",
		"test": "npm run build && mocha dist/test",
		"test:unit": "npm run build && mocha dist/test/unit",
		"test:integration": "npm run build && mocha dist/test/integration",
		"migrate:make": "npm run build && knex migrate:make --knexfile dist/knexfile.js",
		"migrate:latest": "npm run build && knex migrate:latest --knexfile dist/knexfile.js",
		"migrate:rollback": "npm run build && knex migrate:rollback --knexfile dist/knexfile.js",
		"preversion": "npm run lint && npm run clean && npm run test",
		"prepublishOnly": "git push && git push --tags"
	},
	"main": "dist/lib/index.js",
	"types": "dist/types/index.d.ts",
	"devDependencies": {
		"@batterii/eslint-config-ts": "^2.1.0",
		"@batterii/generator-ts-class": "^0.2.0",
		"@batterii/generator-ts-module": "^0.2.0",
		"@batterii/generator-ts-test": "^0.1.2",
		"@types/chai": "^4.2.11",
		"@types/chai-as-promised": "^7.1.2",
		"@types/knex": "^0.16.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^12.12.42",
		"@types/sinon": "^7.5.2",
		"@types/sinon-chai": "^3.2.4",
		"@typescript-eslint/eslint-plugin": "^2.34.0",
		"@typescript-eslint/parser": "^2.34.0",
		"chai": "^4.2.0",
		"chai-as-promised": "^7.1.1",
		"eslint": "^6.8.0",
		"knex": "^0.21.1",
		"knex-cleaner": "^1.3.0",
		"mocha": "^7.2.0",
		"objection": "^2.1.5",
		"pg": "^8.2.1",
		"sinon": "^7.5.0",
		"sinon-chai": "^3.5.0",
		"source-map-support": "^0.5.19",
		"typescript": "3.8.3",
		"yo": "^3.1.1"
	},
	"dependencies": {
		"@types/lodash": "^4.14.154",
		"lodash": "^4.17.15"
	}
}
