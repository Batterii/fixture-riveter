{
	"docs/*.md": {"type": "doc"},
	"README.md": {"type": "doc"},
	"lib/*.ts": {
		"alternate": ["test/acceptance/{}.ts","test/unit/{}.ts"],
		"type": "source"
	},
	"test/acceptance/*.ts": {
		"alternate": "lib/{}.ts",
		"type": "test"
	},
	"test/unit/*.ts": {
		"alternate": "lib/{}.ts",
		"type": "unit"
	}
}
